{% extends 'base.html.twig' %}

{% block content %}
<div class="container">
    <div class="row">
        <div class="col-sm-12">
           <h3>Articles</h3>
            <table class="table">
                {% for article in articles %}
                    <tr>
                        <td>{{article['title']}}</td>
                        <td>{{article['content']}}</td>
                    </tr>
                {% endfor %}
            </table>

        </div>

    </div>
</div>
{% endblock %}